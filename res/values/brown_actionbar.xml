<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="MyActionBar" parent="@style/Widget.AppCompat.Light.ActionBar.Solid.Inverse">
    	<item name="android:background"  tools:ignore="NewApi">@color/brown</item>
    	<item name="background">@color/brown</item>
	</style>

	<style name="CustomActionBarTheme" parent="@style/Theme.AppCompat.Light.DarkActionBar">
    	<item name="android:actionBarStyle" tools:ignore="NewApi">@style/MyActionBar</item>
    	<item name="actionBarStyle">@style/MyActionBar</item>
    	
    	<item name="colorPrimary">@color/brown</item> 
		<item name="colorPrimaryDark">@color/dark_brown</item>
		<item name="colorAccent">@color/orange</item>
		<item name="colorControlNormal">@color/dark_brown</item>
    	<item name="colorControlActivated">@color/orange</item>
    	<item name="colorControlHighlight">@color/orange</item>
	</style>

</resources>